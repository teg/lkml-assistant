  "dependencies": {
    "aws-cdk": "^2.1012.0",
    "aws-cdk-lib": "^2.192.0",
    "constructs": "^10.4.2",
    "source-map-support": "^0.5.21"
  },
  "devDependencies": {
    "@types/node": "^22.15.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  }
